--player--

function iplr()
 plr={
  x=63,
  y=63
}
end

  --movements--
  
function uplr()
 if btn(➡️) then
  plr.x+=1
  elseif btn(⬅️) then
  plr.x-=1
  elseif btn(⬆️) then 
  plr.y-=1
  elseif btn(⬇️) then
  plr.y+=1
 end
 
 --plant seeds--
 local ptx=(plr.x+4)/8
 local pty=(plr.y+7)/8
 
 if btnp(❎) then
 mset(ptx,pty,3)
 add (seeds,{
     sx=ptx,
     sy=pty,
     tig=0 -- time in grow--
     })
 end

end

function dplr()
 spr(12,plr.x,plr.y)
end


 
 




